{
  "version": 2,
  "builds": [
    {
      "src": "cloud-api/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cloud-api/main.py"
    }
  ],
  "functions": {
    "cloud-api/main.py": {
      "includeFiles": "cloud-api/**"
    }
  }
} 